<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:mainApplication="clr-namespace:BankManager.ViewModels.MainApplication"
             xmlns:commonControls="clr-namespace:BankManager.Views.CommonControls"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="BankManager.Views.MainApplication.TransactionsView">

    <Design.DataContext>
        <!-- ReSharper disable once Xaml.ConstructorWarning -->
        <mainApplication:TransactionsViewModel/>
    </Design.DataContext>
    
    <StackPanel Orientation="Vertical">
        <commonControls:PageHeader HeaderText="{Binding PageHeaderText}" />
    </StackPanel>
    
    
    
    
</UserControl>
